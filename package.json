{
  "name": "fs_snapshot",
  "productName": "FSSnapshot",
  "version": "1.0.0",
  "description": "Applications for creating and comparing snapshots",
  "main": ".webpack/main",
  "build": {
    "appId": "com.elegerd.fs_snapshot"
  },
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish"
  },
  "keywords": [],
  "author": {
    "name": "Kravtsov Maksim",
    "email": "kravcov.max@gmail.com"
  },
  "license": "ISC",
  "config": {
    "forge": {
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "fs_snapshot"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "electronPackagerConfig"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/renderer/index.html",
                  "js": "./src/renderer/index.jsx",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "@electron-forge/cli": "^6.0.0-beta.50",
    "@electron-forge/maker-deb": "^6.0.0-beta.50",
    "@electron-forge/maker-rpm": "^6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.50",
    "@electron-forge/maker-zip": "^6.0.0-beta.50",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.50",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "babel-loader": "^8.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.4.2",
    "electron": "22.3.25",
    "electron-devtools-installer": "^3.0.0",
    "file-loader": "^6.0.0",
    "node-loader": "^0.6.0",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.42.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "@material-ui/core": "^4.9.11",
    "async": "^3.2.0",
    "classnames": "^2.2.6",
    "cron": "^1.8.2",
    "crypto-js": "^4.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "electron-store": "^5.1.1",
    "excel4node": "^1.7.2",
    "hasha": "^5.2.0",
    "promise-limit": "^2.7.0",
    "rc-progress": "^2.5.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-simple-timefield": "^3.0.0",
    "react-switch": "^5.0.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  }
}
